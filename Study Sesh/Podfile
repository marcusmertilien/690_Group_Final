target 'Study Sesh' do

  # Comment the next line if you're not using Swift and don't want to use dynamic 
source 'https://github.com/CocoaPods/Specs.git'
  platform :ios, '11.2'
  use_frameworks!

  

  # Pods for Study Sesh
  pod 'Firebase'
  pod 'Firebase/Auth'
  pod 'Firebase/Analytics'
  pod 'Firebase/AppIndexing'
  pod 'Firebase/Core'
  pod 'Firebase/Crash'
  pod 'Firebase/Database'
  pod 'Firebase/DynamicLinks'
  pod 'Firebase/Messaging'
  pod 'Firebase/RemoteConfig'
  pod 'Firebase/Storage'
  pod 'JSQMessagesViewController'
  pod 'JTAppleCalendar', '~> 7.0'
  
  
  target 'Study SeshTests' do
      pod 'Firebase'
      pod 'Firebase/Auth'
      pod 'Firebase/Analytics'
      pod 'Firebase/AppIndexing'
      pod 'Firebase/Core'
      pod 'Firebase/Crash'
      pod 'Firebase/Database'
      pod 'Firebase/DynamicLinks'
      pod 'Firebase/Messaging'
      pod 'Firebase/RemoteConfig'
      pod 'Firebase/Storage'
      pod 'JSQMessagesViewController'
      pod 'JTAppleCalendar', '~> 7.0'
    inherit! :search_paths
    # Pods for testing
  end

  target 'Study SeshUITests' do
      pod 'Firebase'
      pod 'Firebase/Auth'
      pod 'Firebase/Analytics'
      pod 'Firebase/AppIndexing'
      pod 'Firebase/Core'
      pod 'Firebase/Crash'
      pod 'Firebase/Database'
      pod 'Firebase/DynamicLinks'
      pod 'Firebase/Messaging'
      pod 'Firebase/RemoteConfig'
      pod 'Firebase/Storage'
      pod 'JSQMessagesViewController'
      pod 'JTAppleCalendar', '~> 7.0'

    inherit! :search_paths
    # Pods for testing
  end

end